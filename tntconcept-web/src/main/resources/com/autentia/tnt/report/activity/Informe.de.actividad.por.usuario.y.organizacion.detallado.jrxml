	<queryString><![CDATA[SELECT		    o.name as OrgName,
		    u.name as userName,
		    p.name as projectName,
		    r.name as roleName,
                    p.id as idProyecto,
                    r.id as idRol
FROM Activity a,Organization o,User u,Project p,ProjectRole r
where
u.id=a.userId and
r.id=a.roleId and
p.organizationId=$P{Organizacion} and
p.id=r.projectId and
u.id=$P{Usuario} and
a.startDate between $P{Fecha inicio} and $P{Fecha fin}
group by a.roleId
order by o.name,a.startDate,u.name,p.name;]]></queryString>

