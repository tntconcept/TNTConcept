	<queryString><![CDATA[select orgName,userName,roleName,start,duration,description,
		COALESCE(fact/60,0) as fact,
		COALESCE(noFact/60,0) as noFact,
		COALESCE(impFact/60,0) as impFact,
 		projectName from (

			SELECT o.name as orgName,u.name as userName,r.name as roleName,p.name as projectName,
				a.start,a.duration,a.description,
				sum(CASE WHEN a.billable=1 then duration end) as 'fact',
				sum(CASE WHEN a.billable=0 then duration end) as 'noFact',
        			sum(CASE WHEN a.billable=1 then (duration * r.costPerHour) end) as 'impFact'
		FROM Activity a,Organization o,User u,Project p,ProjectRole r 
			where u.id=a.userId and r.id=a.roleId and p.organizationId=o.id and p.id=r.projectId 
			and p.id=$P{Proyecto}  and a.start between $P{Fecha inicio} and $P{Fecha fin}

		group by a.id
		order by a.start,u.name) as tabla;]]></queryString>

