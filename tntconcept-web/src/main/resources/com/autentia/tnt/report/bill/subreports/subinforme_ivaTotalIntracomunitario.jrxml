	<parameter name="Fecha inicio" isForPrompting="true" class="java.util.Date">
		<parameterDescription><![CDATA[date]]></parameterDescription>
		<defaultValueExpression ><![CDATA[new Date()]]></defaultValueExpression>
	</parameter>
	<parameter name="Fecha fin" isForPrompting="true" class="java.util.Date">
		<parameterDescription><![CDATA[date]]></parameterDescription>
		<defaultValueExpression ><![CDATA[new Date()]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SELECT ivaIntra.totalIvaIntracomunitario, ivaDesgravable.totalIvaDesgravable,
(ivaIntra.totalIvaIntracomunitario + ivaDesgravable.totalIvaDesgravable) AS sumaIntracomunitarioDesgravable
FROM (SELECT sum((bb.units*bb.amount)*((bb.ivaOnlySII/100))) as totalIvaIntracomunitario
FROM Bill AS b, BillBreakDown AS bb
WHERE bb.billId =  b.id
AND b.billType ='RECIEVED'
AND b.billRegimeId = 25  -- ADQUISICIONES INTRACOMUNITARIAS
AND b.creationDate between $P{Fecha inicio} and $P{Fecha fin}) AS ivaIntra,
(SELECT sum((bb.units*bb.amount)*((bb.iva/100))*((b.deductibleIVAPercentage/100))) as totalIvaDesgravable
FROM Bill AS b, BillBreakDown AS bb
WHERE bb.billId = b.id
AND b.billType = 'RECIEVED'
AND b.creationDate between $P{Fecha inicio} and $P{Fecha fin}) AS ivaDesgravable;]]></queryString>

