	select b.id from Bill b, Organization o
	where b.billType='recieved'
	and b.providerId = o.id
	and b.billRegimeId = 25
	and b.creationDate between $P{Fecha inicio} and $P{Fecha fin}
) group by iva order by iva desc;]]></queryString>

