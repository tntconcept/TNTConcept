	<queryString><![CDATA[SELECT bb.concept as concepto,b.id as billId,o.documentNumber as documentNumber, case b.state when 'PAID' then 'Pagada'
                    when 'EMITTED' then 'Emitida'
                    when 'IN_CREDITTITLE' then 'En pagare'
                    end as estado,b.number,o.name,b.creationDate
	,sum((bb.units*bb.amount)) as baseImponible, sum((bb.units*bb.amount)*((bb.iva/100))) as iva,
	sum((bb.units*bb.amount)*(1+(bb.iva/100))) as sumaFactura, bb.iva as percent
from Bill b left join BillBreakDown bb on b.id=bb.billId, Organization o
where b.billType='recieved'
and bb.billId=b.id and b.providerId = o.id
and b.creationDate between $P{Fecha inicio} and $P{Fecha fin}
group by number
order by number asc, b.creationDate asc]]></queryString>

