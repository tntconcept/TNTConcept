	<queryString><![CDATA[SELECT distinct aet.name,
(select sum(sub_ae.amount)
 from AccountEntryType sub_aet, AccountEntry sub_ae 
 where sub_aet.id = sub_ae.accountEntryTypeId
 and sub_ae.entryDate between $P{Fecha inicio} and $P{Fecha fin} 
 and sub_aet.id = aet.id
) as gastoActual,
(select sum(sub_ae.amount) 
 from AccountEntryType sub_aet, AccountEntry sub_ae 
 where sub_aet.id = sub_ae.accountEntryTypeId
 and sub_ae.entryDate between DATE_SUB($P{Fecha inicio},INTERVAL 1 YEAR) and DATE_SUB($P{Fecha fin},INTERVAL 1 YEAR)
 and sub_aet.id = aet.id
) as gastoPasado
FROM AccountEntryType aet, AccountEntry ae 
where aet.id = ae.accountEntryTypeId 
and aet.accountEntryGroupId = 2
and ((ae.entryDate between $P{Fecha inicio} and $P{Fecha fin}) or (ae.entryDate between DATE_SUB($P{Fecha inicio},INTERVAL 1 YEAR) and DATE_SUB($P{Fecha fin},INTERVAL 1 YEAR)))
order by name;
]]></queryString>

