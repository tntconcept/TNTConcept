	<queryString><![CDATA[select orgName,userName,roleName,startDate,duration,description, COALESCE(fact,0) as fact, COALESCE(noFact,0) as noFact from (SELECT o.name as orgName,u.name as userName,
 r.name as roleName,a.startDate,duration,a.description,sum(CASE WHEN a.billable=1
   					       then duration end) as 'fact',
				      sum(CASE WHEN a.billable=0
				      then duration end) as 'noFact' 
FROM Activity a,Organization o,User u,Project p,ProjectRole r 
where u.id=a.userId and r.id=a.roleId and p.organizationId=o.id and p.id=r.projectId 
and u.id=$P{Usuario} and a.startDate between $P{Fecha inicio} and $P{Fecha fin}
group by a.roleId
order by o.name,a.startDate,u.name) as tabla;]]></queryString>

