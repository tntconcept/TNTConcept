	<queryString><![CDATA[select userName,orgName,proName,roleName,
       COALESCE(fact/60,0) as fact,
       COALESCE(noFact/60,0) as noFact,
       COALESCE(fact/60,0)+COALESCE(noFact/60,0) as total
from (SELECT o.name as orgName,u.name as userName,p.name as proName,r.name as roleName,
      sum(CASE WHEN a.billable=1 then duration end) as 'fact',
      sum(CASE WHEN a.billable=0 then duration end) as 'noFact' 
FROM Activity a,Organization o,User u,Project p,ProjectRole r 
where u.id=a.userId and r.id=a.roleId and p.organizationId=o.id and p.id=r.projectId 
and u.id=$P{Usuario} and a.startDate between $P{Fecha inicio} and $P{Fecha fin}
group by a.roleId
order by o.name, p.name, r.name) as tabla;]]></queryString>

