	<queryString><![CDATA[SELECT 
    login, uname, ucname, orgName, projName, roleName
FROM
    (SELECT 
        User.id,
            User.login,
            User.name AS uname,
            UserCategory.name AS ucname
    FROM
        User
    INNER JOIN UserCategory ON User.categoryId = UserCategory.id) u
        INNER JOIN
    (SELECT 
        userId, orgName, projName, roleName, startDate
    FROM
        Activity a
    INNER JOIN (SELECT 
        p.id, orgName, projName, p.name as roleName
    FROM
        ProjectRole p
    INNER JOIN (SELECT 
        Project.id,
            Organization.name AS orgName,
            Project.name AS projName
    FROM
        Project
    INNER JOIN Organization ON organizationId = Organization.id) s ON p.projectId = s.id) r ON r.id = a.roleId) o ON u.id = o.userId
		WHERE startDate between $P{Fecha inicio} and $P{Fecha fin}
		GROUP BY login, orgName, projName, roleName 
		ORDER BY uname, startDate;]]></queryString>

