<%@page language="java" contentType="text/html; charset=UTF-8"%>

<%@include file="/inc/tlibs.jsp" %>

<html>
  <head>
    <%@include file="/inc/uiCore.jsp" %>
  </head>	
  <body>
    
    <!-- searchTutorial.jsp: generated by stajanov code generator -->
    <f:loadBundle basename="com.autentia.tnt.resources.messages" var="msg" />
    <i:location place="searchTutorial" msg="${msg}"/> 
    
    <f:view>
      <%@include file="/inc/header.jsp" %>
      <h:form id="search" enctype="multipart/form-data">
        
        <%-- Header --%>
        <i:titleBar msg="${msg}">
          <h:commandLink action="#{tutorialBean.list}">
            <h:graphicImage title="#{msg.entityActions_run}"  value="/img/run.png" styleClass="titleImg" />
          </h:commandLink>
        </i:titleBar>
        
        <%-- Search form --%>
        <table class="searchTable" cellpadding="0" cellspacing="0">

          
      <%-- Ignored field: id --%>

                        
      <%-- Field: name --%>
    <tr>
      <td class="searchLabel">${msg['tutorial.name']}:</td>
      <td class="searchFieldCell">

        
          
          <h:panelGroup>
            <h:message styleClass="error" showSummary="true" showDetail="false" for="name" />
            <h:selectBooleanCheckbox id="nameValid" value="#{tutorialBean.searchNameValid}" 
                                      onclick="setEnabled('search:name',this.checked)"/>
            <h:inputText id="name" value="#{tutorialBean.searchName}" size="70"/>
          </h:panelGroup>
          <script>
            setEnabled( 'search:name', ${tutorialBean.searchNameValid} )
          </script>

        
      </td>
    </tr>
                        
      <%-- Field: description --%>
    <tr>
      <td class="searchLabel">${msg['tutorial.description']}:</td>
      <td class="searchFieldCell">

        
          
          <h:panelGroup>
            <h:message styleClass="error" showSummary="true" showDetail="false" for="description" />
            <h:selectBooleanCheckbox id="descriptionValid" value="#{tutorialBean.searchDescriptionValid}" 
                                      onclick="setEnabled('search:description',this.checked)"/>
            <h:inputText id="description" value="#{tutorialBean.searchDescription}" size="70"/>
          </h:panelGroup>
          <script>
            setEnabled( 'search:description', ${tutorialBean.searchDescriptionValid} )
          </script>

        
      </td>
    </tr>
                        
      <%-- Field: maxDeliveryDate --%>
    <tr>
      <td class="searchLabel">${msg['tutorial.maxDeliveryDate']}:</td>
      <td class="searchFieldCell">

        
                    
          <h:panelGroup>
            <h:message styleClass="error" showSummary="true" showDetail="false" for="startMaxDeliveryDate" />
            <h:selectBooleanCheckbox id="startMaxDeliveryDateValid" value="#{tutorialBean.searchStartMaxDeliveryDateValid}" 
                                      onclick="setEnabled('search:startMaxDeliveryDate',this.checked)"/>
            <t:inputCalendar id="startMaxDeliveryDate" value="#{tutorialBean.searchStartMaxDeliveryDate}"
                             renderAsPopup="true" popupDateFormat="d/MM/yyyy" renderPopupButtonAsImage="true" 
                             popupTodayString="#{msg['calendar.today']}" popupWeekString="#{msg['calendar.week']}"
                             onchange="setEnabled('search:startMaxDeliveryDate',true);setChecked('search:startMaxDeliveryDateValid',true);"
                             />
          </h:panelGroup>
          -
          <h:panelGroup>
            <h:message styleClass="error" showSummary="true" showDetail="false" for="endMaxDeliveryDate" />
            <h:selectBooleanCheckbox id="endMaxDeliveryDateValid" value="#{tutorialBean.searchEndMaxDeliveryDateValid}" 
                                      onclick="setEnabled('search:endMaxDeliveryDate',this.checked)"/>
            <t:inputCalendar id="endMaxDeliveryDate" value="#{tutorialBean.searchEndMaxDeliveryDate}"
                             renderAsPopup="true" popupDateFormat="d/MM/yyyy" renderPopupButtonAsImage="true" 
                             popupTodayString="#{msg['calendar.today']}" popupWeekString="#{msg['calendar.week']}"
                             onchange="setEnabled('search:endMaxDeliveryDate',true);setChecked('search:endMaxDeliveryDateValid',true);"
                             />
          </h:panelGroup>
          <script>
            setEnabled( 'search:startMaxDeliveryDate', ${tutorialBean.searchStartMaxDeliveryDateValid} )
            setEnabled( 'search:endMaxDeliveryDate', ${tutorialBean.searchEndMaxDeliveryDateValid} )
          </script>

        
      </td>
    </tr>
                        
      <%-- Field: endDate --%>
    <tr>
      <td class="searchLabel">${msg['tutorial.endDate']}:</td>
      <td class="searchFieldCell">

        
                    
          <h:panelGroup>
            <h:message styleClass="error" showSummary="true" showDetail="false" for="startEndDate" />
            <h:selectBooleanCheckbox id="startEndDateValid" value="#{tutorialBean.searchStartEndDateValid}" 
                                      onclick="setEnabled('search:startEndDate',this.checked)"/>
            <t:inputCalendar id="startEndDate" value="#{tutorialBean.searchStartEndDate}"
                             renderAsPopup="true" popupDateFormat="d/MM/yyyy" renderPopupButtonAsImage="true" 
                             popupTodayString="#{msg['calendar.today']}" popupWeekString="#{msg['calendar.week']}"
                             onchange="setEnabled('search:startEndDate',true);setChecked('search:startEndDateValid',true);"
                             />
          </h:panelGroup>
          -
          <h:panelGroup>
            <h:message styleClass="error" showSummary="true" showDetail="false" for="endEndDate" />
            <h:selectBooleanCheckbox id="endEndDateValid" value="#{tutorialBean.searchEndEndDateValid}" 
                                      onclick="setEnabled('search:endEndDate',this.checked)"/>
            <t:inputCalendar id="endEndDate" value="#{tutorialBean.searchEndEndDate}"
                             renderAsPopup="true" popupDateFormat="d/MM/yyyy" renderPopupButtonAsImage="true" 
                             popupTodayString="#{msg['calendar.today']}" popupWeekString="#{msg['calendar.week']}"
                             onchange="setEnabled('search:endEndDate',true);setChecked('search:endEndDateValid',true);"
                             />
          </h:panelGroup>
          <script>
            setEnabled( 'search:startEndDate', ${tutorialBean.searchStartEndDateValid} )
            setEnabled( 'search:endEndDate', ${tutorialBean.searchEndEndDateValid} )
          </script>

        
      </td>
    </tr>
                        
      <%-- Ignored field: departmentId --%>

                        
      <%-- Ignored field: insertDate --%>

                        
      <%-- Ignored field: updateDate --%>

                                  
      <%-- Field: user --%>
    <tr>
      <td class="searchLabel">${msg['tutorial.user']}:</td>
      <td class="searchFieldCell">

        
          
          <h:panelGroup>
            <h:message styleClass="error" showSummary="true" showDetail="false" for="user" />
            <h:selectBooleanCheckbox id="userValid" value="#{tutorialBean.searchUserValid}" 
                                      onclick="setEnabled('search:user',this.checked)"/>
            <h:selectOneMenu id="user" value="#{tutorialBean.searchUser}" onclick="setEnabled('search:user',true)">
              <f:selectItems value="#{tutorialBean.users}" />
              <f:converter converterId="autentia.EntityConverter"/>
            </h:selectOneMenu>
          </h:panelGroup>
          <script>
            setEnabled( 'search:user', ${tutorialBean.searchUserValid} )
          </script>

        
      </td>
    </tr>
                                
        </table>
        
      </h:form>
    </f:view>
    
  </body>
</html>  		
