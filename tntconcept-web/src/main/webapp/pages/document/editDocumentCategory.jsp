<%@page language="java" contentType="text/html; charset=UTF-8"%>

<%@include file="/inc/tlibs.jsp" %>

<html>
  <head>
    <%@include file="/inc/uiCore.jsp" %>
  </head>	
  <body>
    
    <!-- editDocumentCategory.jsp: generated by stajanov code generator -->
    <f:loadBundle basename="com.autentia.tnt.resources.messages" var="msg" />
    <i:location place="editDocumentCategory" msg="${msg}"/> 
    
    <f:view>
        <%@include file="/inc/header.jsp" %>
      <h:form id="documentCategory" enctype="multipart/form-data">
        
        <%-- Header --%>
        <i:titleBar name="${documentCategoryBean.name}" msg="${msg}">
          <h:commandLink action="#{documentCategoryBean.save}" onclick="if(!askSave('#{documentCategoryBean.id}','#{msg['question.confirmSave']}')) return false;">
            <h:graphicImage title="#{msg.entityActions_save}"  value="/img/save.gif" styleClass="titleImg" />
          </h:commandLink>
          <h:commandLink rendered="#{documentCategoryBean.id!=null}" action="#{documentCategoryBean.delete}" onclick="if( !confirm('#{msg['question.confirmDelete']}') ) return false;">
            <h:graphicImage title="#{msg.entityActions_delete}"  value="/img/delete.gif" styleClass="titleImg" />
          </h:commandLink>
          <h:commandLink action="#{documentCategoryBean.list}" immediate="true">
            <h:graphicImage title="#{msg.entityActions_back}"  value="/img/back.gif" styleClass="titleImg" />
          </h:commandLink>
        </i:titleBar>      
        
        <%-- Edition form --%>
        <table class="editTable" cellpadding="0" cellspacing="0">
<%-- documentCategory - generated by stajanov (do not edit/delete) --%>







              
  
      <%-- Ignored field: id --%>
  
                              
    
      <%-- Field: name --%>
    <tr>
    	    		<td class="editLabelRW">*${msg['documentCategory.name']}:</td>
    	      
      <td class="editFieldCell">

                  <h:panelGroup>
            <h:message styleClass="error" showSummary="true" showDetail="false" for="name" />
                		<h:inputText id="name" value="#{documentCategoryBean.name}" size="70" maxlength="255" required="true" styleClass="requiredFieldClass"/>
    	              </h:panelGroup>

              </td>
    </tr>
                              
  
      <%-- Field: description --%>
    <tr>
    	    		<td class="editLabelRW">${msg['documentCategory.description']}:</td>
    	      
      <td class="editFieldCell">

                  <h:panelGroup>
            <h:message styleClass="error" showSummary="true" showDetail="false" for="description" />
            <h:inputTextarea id="description" value="#{documentCategoryBean.description}" rows="5" cols="68"/>
          </h:panelGroup>

              </td>
    </tr>
                              
  
      <%-- Field: code --%>
    <tr>
    	    		<td class="editLabelRW">${msg['documentCategory.code']}:</td>
    	      
      <td class="editFieldCell">

                  <h:panelGroup>
            <h:message styleClass="error" showSummary="true" showDetail="false" for="code" />
                		<h:inputText id="code" value="#{documentCategoryBean.code}" maxlength="45"/>
    	              </h:panelGroup>

              </td>
    </tr>
                              
  
      <%-- Ignored field: documentsLastUpdate --%>
  
                              
  
      <%-- Ignored field: ownerId --%>
  
                              
  
      <%-- Ignored field: departmentId --%>
  
                              
  
      <%-- Ignored field: insertDate --%>
  
                              
  
      <%-- Ignored field: updateDate --%>
  
                                          
  
      <%-- Field: padre --%>
    <tr>
    	    		<td class="editLabelRW">${msg['documentCategory.padre']}:</td>
    	      
      <td class="editFieldCell">

                  <h:panelGroup>
            <h:message styleClass="error" showSummary="true" showDetail="false" for="padre" />
            <h:selectOneMenu id="padre" value="#{documentCategoryBean.padre}" >
              <f:selectItems value="#{documentCategoryBean.padres}" />
              <f:converter converterId="autentia.EntityConverter"/>
            </h:selectOneMenu>
          </h:panelGroup>

              </td>
    </tr>
                                      

<%-- documentCategory - generated by stajanov (do not edit/delete) --%>
          
          

        </table>
        
      </h:form>
    </f:view>
    
  </body>
</html>  		








            
      
                                      



