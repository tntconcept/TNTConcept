<%@page language="java" contentType="text/html; charset=UTF-8"%>

<%@include file="/inc/tlibs.jsp" %>

<html>
  <head>
    <%@include file="/inc/uiCore.jsp" %>
  </head>	
  <body>
    
    <!-- searchOccupation.jsp: generated by stajanov code generator -->
    <f:loadBundle basename="com.autentia.tnt.resources.messages" var="msg" />
    <i:location place="searchOccupation" msg="${msg}"/> 
    
    <f:view>
      <%@include file="/inc/header.jsp" %>
      <h:form id="search" enctype="multipart/form-data">
        
        <%-- Header --%>
        <i:titleBar msg="${msg}">
          <h:commandLink action="#{occupationBean.list}">
            <h:graphicImage title="#{msg.entityActions_run}"  value="/img/run.png" styleClass="titleImg" />
          </h:commandLink>
        </i:titleBar>
        
        <%-- Search form --%>
        <table class="searchTable" cellpadding="0" cellspacing="0">

          
      <%-- Ignored field: id --%>

                        
      <%-- Field: startDate --%>

    <tr>
      <td class="searchLabel">${msg['occupation.startDate']}:</td>
      <td class="searchFieldCell">
                
          <h:panelGroup>
            <h:message styleClass="error" showSummary="true" showDetail="false" for="startStartDate" />
            <h:selectBooleanCheckbox id="startStartDateValid" value="#{occupationBean.searchStartStartDateValid}" 
                                      onclick="setEnabled('search:startStartDate',this.checked)"/>
            <t:inputCalendar id="startStartDate" value="#{occupationBean.searchStartStartDate}"
                             renderAsPopup="true" popupDateFormat="d/MM/yyyy" renderPopupButtonAsImage="true" 
                             popupTodayString="#{msg['calendar.today']}" popupWeekString="#{msg['calendar.week']}"
                             onchange="setEnabled('search:startStartDate',true);setChecked('search:startStartDateValid',true);"
                             >
				<f:validator validatorId="autentia.dateValidator"/>
			</t:inputCalendar>
          </h:panelGroup>
          -
          <h:panelGroup>
            <h:message styleClass="error" showSummary="true" showDetail="false" for="startEndDate" />
            <h:selectBooleanCheckbox id="startEndDateValid" value="#{occupationBean.searchStartEndDateValid}" 
                                      onclick="setEnabled('search:startEndDate',this.checked)"/>
            <t:inputCalendar id="startEndDate" value="#{occupationBean.searchStartEndDate}"
                             renderAsPopup="true" popupDateFormat="d/MM/yyyy" renderPopupButtonAsImage="true" 
                             popupTodayString="#{msg['calendar.today']}" popupWeekString="#{msg['calendar.week']}"
                             onchange="setEnabled('search:startEndDate',true);setChecked('search:startEndDateValid',true);"
                             >
				<f:validator validatorId="autentia.dateValidator"/>
			</t:inputCalendar>
          </h:panelGroup>
          <script>
            setEnabled( 'search:startStartDate', ${occupationBean.searchStartStartDateValid} )
            setEnabled( 'search:startEndDate', ${occupationBean.searchStartEndDateValid} )
          </script>
        
      </td>
    </tr>

      <%-- Field: endDate --%>
    <tr>
      <td class="searchLabel">${msg['occupation.endDate']}:</td>
      <td class="searchFieldCell">

        
                    
          <h:panelGroup>
            <h:message styleClass="error" showSummary="true" showDetail="false" for="endStartDate" />
            <h:selectBooleanCheckbox id="endStartDateValid" value="#{occupationBean.searchEndStartDateValid}" 
                                      onclick="setEnabled('search:endStartDate',this.checked)"/>
            <t:inputCalendar id="endStartDate" value="#{occupationBean.searchEndStartDate}"
                             renderAsPopup="true" popupDateFormat="d/MM/yyyy" renderPopupButtonAsImage="true" 
                             popupTodayString="#{msg['calendar.today']}" popupWeekString="#{msg['calendar.week']}"
                             onchange="setEnabled('search:endStartDate',true);setChecked('search:endStartDateValid',true);"
                             >
				<f:validator validatorId="autentia.dateValidator"/>
			</t:inputCalendar>
          </h:panelGroup>
          -
          <h:panelGroup>
            <h:message styleClass="error" showSummary="true" showDetail="false" for="endEndDate" />
            <h:selectBooleanCheckbox id="endEndDateValid" value="#{occupationBean.searchEndEndDateValid}" 
                                      onclick="setEnabled('search:endEndDate',this.checked)"/>
            <t:inputCalendar id="endEndDate" value="#{occupationBean.searchEndEndDate}"
                             renderAsPopup="true" popupDateFormat="d/MM/yyyy" renderPopupButtonAsImage="true" 
                             popupTodayString="#{msg['calendar.today']}" popupWeekString="#{msg['calendar.week']}"
                             onchange="setEnabled('search:endEndDate',true);setChecked('search:endEndDateValid',true);"
                             >
				<f:validator validatorId="autentia.dateValidator"/>
			</t:inputCalendar>
          </h:panelGroup>
          <script>
            setEnabled( 'search:endStartDate', ${occupationBean.searchEndStartDateValid} )
            setEnabled( 'search:endEndDate', ${occupationBean.searchEndEndDateValid} )
          </script>
      </td>
    </tr>

    <%-- Field: duration --%>
    <tr>
      <td class="searchLabel">${msg['occupation.duration']}:</td>
      <td class="searchFieldCell">

        
          
          <h:panelGroup>
            <h:message styleClass="error" showSummary="true" showDetail="false" for="startDuration" />
            <h:selectBooleanCheckbox id="startDurationValid" value="#{occupationBean.searchStartDurationValid}" 
                                      onclick="setEnabled('search:startDuration',this.checked)"/>
            <h:inputText id="startDuration" value="#{occupationBean.searchStartDuration}" size="6">
            <f:converter converterId="autentia.MinuteToHourConverter"/>
            </h:inputText>
          </h:panelGroup>
          -
          <h:panelGroup>
            <h:message styleClass="error" showSummary="true" showDetail="false" for="endDuration" />
            <h:selectBooleanCheckbox id="endDurationValid" value="#{occupationBean.searchEndDurationValid}" 
                                      onclick="setEnabled('search:endDuration',this.checked)"/>
            <h:inputText id="endDuration" value="#{occupationBean.searchEndDuration}" size="6">
            <f:converter converterId="autentia.MinuteToHourConverter"/>
            </h:inputText>
          </h:panelGroup>
          <script>
            setEnabled( 'search:startDuration', ${occupationBean.searchStartDurationValid} )
            setEnabled( 'search:endDuration', ${occupationBean.searchEndDurationValid} )
          </script>

        
      </td>
    </tr>
                        
      <%-- Ignored field: description --%>

                        
      <%-- Ignored field: ownerId --%>

                        
      <%-- Ignored field: departmentId --%>

                        
      <%-- Ignored field: insertDate --%>

                        
      <%-- Ignored field: updateDate --%>

                                  
      <%-- Field: user --%>
    <tr>
      <td class="searchLabel">${msg['occupation.user']}:</td>
      <td class="searchFieldCell">

        
          
          <h:panelGroup>
            <h:message styleClass="error" showSummary="true" showDetail="false" for="user" />
            <h:selectBooleanCheckbox id="userValid" value="#{occupationBean.searchUserValid}" 
                                      onclick="setEnabled('search:user',this.checked)"/>
            <h:selectOneMenu id="user" value="#{occupationBean.searchUser}" onclick="setEnabled('search:user',true)">
              <f:selectItems value="#{occupationBean.users}" />
              <f:converter converterId="autentia.EntityConverter"/>
            </h:selectOneMenu>
          </h:panelGroup>
          <script>
            setEnabled( 'search:user', ${occupationBean.searchUserValid} )
          </script>

        
      </td>
    </tr>
             
      <%-- Field: project --%>
    <tr>
      <td class="searchLabel">${msg['occupation.organization']} / ${msg['occupation.project']}:</td>
      <td class="searchFieldCell">

        
          
          <h:panelGroup>
            <h:message styleClass="error" showSummary="true" showDetail="false" for="project" />
            <h:selectBooleanCheckbox id="projectValid" value="#{occupationBean.searchProjectValid}" 
                                      onclick="setEnabled('search:organization',this.checked);setEnabled('search:project',this.checked)"/>
            <h:selectOneMenu id="organization" value="#{occupationBean.selectedOrganization}" immediate="true" onchange="submit()"
                     valueChangeListener="#{occupationBean.onSelectedOrganizationSearchChanged}" onclick="setEnabled('search:organization',true);setEnabled('search:project',true)">
              <f:selectItems value="#{occupationBean.organizations}" />
              <f:converter converterId="autentia.EntityConverter"/>
            </h:selectOneMenu>
            <h:selectOneMenu id="project" value="#{occupationBean.searchProject}" onclick="setEnabled('search:organization',true);setEnabled('search:project',true)">
              <f:selectItems value="#{occupationBean.projectsBySelectedOrganization}" />
              <f:converter converterId="autentia.EntityConverter"/>
            </h:selectOneMenu>
          </h:panelGroup>
          <script>
          	setEnabled( 'search:project', ${occupationBean.searchProjectValid} );
            setEnabled( 'search:organization', ${occupationBean.searchProjectValid} );
          </script>

        
      </td>
    </tr> 
             
                        
     
                                
        </table>
        
      </h:form>
    </f:view>
    
  </body>
</html>  		
