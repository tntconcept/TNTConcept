<%@page language="java" contentType="text/html; charset=UTF-8"%>

<%@include file="/inc/tlibs.jsp" %>

<html>
  <head>
    <%@include file="/inc/uiCore.jsp" %>
  </head>	
  <body>
    
    <!-- editOccupation.jsp: generated by stajanov code generator -->
    <f:loadBundle basename="com.autentia.tnt.resources.messages" var="msg" />
    <i:location place="detailOccupation" msg="${msg}"/> 
    
    <f:view>
      <%@include file="/inc/header.jsp" %>
      <h:form id="occupation" enctype="multipart/form-data">
        
        <%-- Header --%>
        <i:titleBar name="${occupationBean.user.name}" msg="${msg}">
          <t:commandLink action="#{occupationBean.edit}" immediate="true" rendered="#{occupationBean.editAvailable}">
            <f:param name="rowid" value="#{occupationBean.id}" />
            <h:graphicImage title="#{msg.entityActions_edit}"  value="/img/edit.gif" styleClass="cmdImg" />
          </t:commandLink>
          <h:commandLink action="#{occupationBean.delete}" rendered="#{occupationBean.deleteAvailable}" onclick="if( !confirm('#{msg['question.confirmDelete']}') ) return false;">
            <h:graphicImage title="#{msg.entityActions_delete}"  value="/img/delete.gif" styleClass="titleImg" />
          </h:commandLink>
          <h:commandLink action="#{occupationBean.list}" immediate="true">
            <h:graphicImage title="#{msg.entityActions_back}"  value="/img/back.gif" styleClass="titleImg" />
          </h:commandLink>
        </i:titleBar>


        <%-- Detail form --%>
        <table class="detailTable" cellpadding="0" cellspacing="0">

            
    
  
  
      <%-- Field: id --%>
    <tr>
	<td class="detailLabelRW">${msg['occupation.id']}:</td>
	<td class="detailFieldCell">
         	       <h:outputText value="#{occupationBean.id}" />
 	      </td>
    </tr>
                            
    
  
  
      <%-- Field: startDate --%>
    <tr>
	<td class="detailLabelRW">${msg['occupation.startDate']}:</td>
	<td class="detailFieldCell">
         	    	<h:outputText value="#{occupationBean.startDate}" converter="autentia.dateConverter" />
 	          </td>
    </tr>
                            
    
  
  
      <%-- Field: endDate --%>
    <tr>
	<td class="detailLabelRW">${msg['occupation.endDate']}:</td>
	<td class="detailFieldCell">
         	    	<h:outputText value="#{occupationBean.endDate}" converter="autentia.dateConverter" />
 	          </td>
    </tr>
                            
    
  
  
      <%-- Field: description --%>
    <tr>
	<td class="detailLabelRW">${msg['occupation.description']}:</td>
	<td class="detailFieldCell">
         	       <h:outputText value="#{occupationBean.description}" />
 	      </td>
    </tr>
                            
    
  
  
      <%-- Field: duration --%>
    <tr>
	<td class="detailLabelRW">${msg['occupation.duration']}:</td>
	<td class="detailFieldCell">
         	       <h:outputText value="#{occupationBean.duration}" />
 	      </td>
    </tr>
                            
    
  
  
      <%-- Ignored field: ownerId --%>
  
                            
    
  
  
      <%-- Ignored field: departmentId --%>
  
                            
    
  
  
      <%-- Ignored field: insertDate --%>
  
                            
    
  
  
      <%-- Ignored field: updateDate --%>
  
                                        
    
  
  
      <%-- Field: user --%>
    <tr>
	<td class="detailLabelRW">${msg['occupation.user']}:</td>
	<td class="detailFieldCell">
         	    	<h:outputText value="#{occupationBean.user.name}"/>
 	          </td>
    </tr>
                            
    
  
  
      <%-- Field: project --%>
    <tr>
	<td class="detailLabelRW">${msg['occupation.project']}:</td>
	<td class="detailFieldCell">
         	    	<h:outputText value="#{occupationBean.project.name}"/>
 	          </td>
    </tr>
                                      
        </table>

      </h:form>
    </f:view>
    
  </body>
</html>  		

