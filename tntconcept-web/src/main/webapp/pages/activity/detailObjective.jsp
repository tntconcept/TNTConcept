<%@page language="java" contentType="text/html; charset=UTF-8"%>

<%@include file="/inc/tlibs.jsp" %>

<html>
  <head>
    <%@include file="/inc/uiCore.jsp" %>
  </head>	
  <body>
    
    <!-- editObjective.jsp: generated by stajanov code generator -->
    <f:loadBundle basename="com.autentia.tnt.resources.messages" var="msg" />
    <i:location place="detailObjective" msg="${msg}"/> 
    
    <f:view>
        <%@include file="/inc/header.jsp" %>
      <h:form id="objective" enctype="multipart/form-data">
        
        <%-- Header --%>
        <i:titleBar name="${objectiveBean.name}" msg="${msg}">
          <t:commandLink action="#{objectiveBean.edit}" immediate="true" rendered="#{objectiveBean.editAvailable}">
            <f:param name="rowid" value="#{objectiveBean.id}" />
            <h:graphicImage title="#{msg.entityActions_edit}"  value="/img/edit.gif" styleClass="cmdImg" />
          </t:commandLink>
          <h:commandLink action="#{objectiveBean.delete}" rendered="#{objectiveBean.deleteAvailable}" onclick="if( !confirm('#{msg['question.confirmDelete']}') ) return false;">
            <h:graphicImage title="#{msg.entityActions_delete}"  value="/img/delete.gif" styleClass="titleImg" />
          </h:commandLink>
          <h:commandLink action="#{objectiveBean.list}" immediate="true">
            <h:graphicImage title="#{msg.entityActions_back}"  value="/img/back.gif" styleClass="titleImg" />
          </h:commandLink>
        </i:titleBar>


        <%-- Detail form --%>
        <table class="detailTable" cellpadding="0" cellspacing="0">

            
  
      <%-- Ignored field: id --%>
  
                            
  
      <%-- Field: state --%>
    <tr>
	<td class="detailLabelRW">${msg['objective.state']}:</td>
	<td class="detailFieldCell">
                	<h:outputText value="#{objectiveBean.stateFormatted}" />
              </td>
    </tr>
                            
  
      <%-- Field: name --%>
    <tr>
	<td class="detailLabelRW">${msg['objective.name']}:</td>
	<td class="detailFieldCell">
         	       <h:outputText value="#{objectiveBean.name}" />
 	      </td>
    </tr>
                            
  
      <%-- Field: log --%>
    <tr>
	<td class="detailLabelRW">${msg['objective.log']}:</td>
	<td class="detailFieldCell">
         	       <h:outputText value="#{objectiveBean.log}" />
 	      </td>
    </tr>
                            
  
      <%-- Field: startDate --%>
    <tr>
	<td class="detailLabelRW">${msg['objective.startDate']}:</td>
	<td class="detailFieldCell">
         	    	<h:outputText value="#{objectiveBean.startDate}" converter="autentia.dateConverter" />
 	          </td>
    </tr>
                            
  
      <%-- Field: endDate --%>
    <tr>
	<td class="detailLabelRW">${msg['objective.endDate']}:</td>
	<td class="detailFieldCell">
         	    	<h:outputText value="#{objectiveBean.endDate}" converter="autentia.dateConverter" />
 	          </td>
    </tr>
                            
  
      <%-- Ignored field: departmentId --%>
  
                            
  
      <%-- Ignored field: insertDate --%>
  
                            
  
      <%-- Ignored field: updateDate --%>
  
                                        
  
      <%-- Field: user --%>
    <tr>
	<td class="detailLabelRW">${msg['objective.user']}:</td>
	<td class="detailFieldCell">
         	    	<h:outputText value="#{objectiveBean.user.name}"/>
 	          </td>
    </tr>
                            
  
      <%-- Field: project --%>
    <tr>
	<td class="detailLabelRW">${msg['objective.project']}:</td>
	<td class="detailFieldCell">
         	    	<h:outputText value="#{objectiveBean.project.name}"/>
 	          </td>
    </tr>
                                      
        </table>

      </h:form>
    </f:view>
    
  </body>
</html>  		
<%-- objective - generated by stajanov (do not edit/delete) --%>
